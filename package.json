{
  "name": "lp-format",
  "displayName": "LP Format",
  "description": "Support for LP files.",
  "icon": "images/polytope.png",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/cabralski/lp-format.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "lp",
      "aliases": ["LP", "lp"],
      "extensions": [".lp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "lp",
      "scopeName": "source.lp",
      "path": "./syntaxes/lp.tmLanguage.json"
    }]
  }
}
